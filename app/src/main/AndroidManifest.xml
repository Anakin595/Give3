<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.give3.gizrog.give3">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" android:maxSdkVersion="27"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/g3_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/g3_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" />
        <activity android:name=".SectionsActivity" />
        <activity android:name=".SectionSquadActivity" />
        <activity android:name=".TasksActivity" />
        <activity android:name=".AssessmentActivity"/>
    </application>

</manifest>